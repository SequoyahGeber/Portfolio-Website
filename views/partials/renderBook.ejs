<div id="viewer"></div>
<script>
  var viewer = document.getElementById("viewer");
  var bookURL = "<%= epubURL %>";
  var book = ePub(bookURL);
  var rendition = book.renderTo("viewer", {
    method: "default",
    width: "100%",
    height: "100%",
  });
  var displayed = rendition.display();
</script>
